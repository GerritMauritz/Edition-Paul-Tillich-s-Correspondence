   <define name="monogr">
      <element name="monogr">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(monographic level) contains bibliographic elements describing an item (e.g. a book or journal) published as an independent item (i.e. as a separate physical object). [3.12.2.1. Analytic, Monographic, and Series Levels]</a:documentation>
         <group>
      
            <optional>
               <choice>
                  <group>
                     <choice>
                        <ref name="author"/>
                        <ref name="editor"/>
              
                        <ref name="respStmt"/>
                     </choice>
            
                     <zeroOrMore>
                        <choice>
                           <ref name="author"/>
                           <ref name="editor"/>
                
                           <ref name="respStmt"/>
                        </choice>
                     </zeroOrMore>
            
            
                     <oneOrMore>
                        <ref name="title"/>
                     </oneOrMore>
            
            
                     <zeroOrMore>
                        <choice>
                           <ref name="model.ptrLike"/>
                           <ref name="idno"/>
                
                           <ref name="editor"/>
                           <ref name="respStmt"/>
                        </choice>
                     </zeroOrMore>
            
                  </group>
                  <group>
            
                     <oneOrMore>
                        <choice>
                           <ref name="title"/>
                           <ref name="model.ptrLike"/>
                           <ref name="idno"/>
                        </choice>
                     </oneOrMore>
            
            
                     <zeroOrMore>
                        <choice>
                
                           <ref name="author"/>
                           <ref name="editor"/>
                
                           <ref name="respStmt"/>
                        </choice>
                     </zeroOrMore>
            
                  </group>
          
            
            
                  <ref name="idno"/>
          
               </choice>
            </optional>
      
      
            <zeroOrMore>
               <ref name="availability"/>
