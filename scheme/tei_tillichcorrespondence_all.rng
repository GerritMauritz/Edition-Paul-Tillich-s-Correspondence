            <zeroOrMore>
               <group>
                  <ref name="edition"/>
        
                  <zeroOrMore>
                     <choice>
                        <ref name="idno"/>
                        <ref name="model.ptrLike"/>
                        <ref name="editor"/>
            
            
                        <ref name="respStmt"/>
